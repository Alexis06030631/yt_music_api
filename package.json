{
	"name": "ytmusic_api_unofficial",
	"version": "1.1.0-beta.1.10.6",
	"description": "A simple API to get music from YouTube Music",
	"scripts": {
		"test": "nodemon test.js",
		"publish": "npm run build && npm publish --access public",
		"build": "tsc",
		"build_auto": "nodemon --watch src --exec \"npm run build\"",
		"docs:build": "node docs/build.js && docsify serve docs"
	},
	"nodemonConfig": {
		"ignore": [
			"*.json"
		],
		"ext": "js,json,ts"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"keywords": [
		"YouTube",
		"YouTube music",
		"music",
		"Free"
	],
	"author": "Alexis06030631",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git@github.com:Alexis06030631/meteofrance_api.git",
		"directory": "src",
		"branch": "main"
	},
	"bugs": {
		"url": "https://github.com/Alexis06030631/ytmusic_api/issues"
	},
	"homepage": "https://github.com/Alexis06030631/ytmusic_api/",
	"dependencies": {
		"axios": "^1.4.0",
		"puppeteer": "^21.1.0"
	},
	"devDependencies": {
		"@types/node": "^20.1.0",
		"fs-extra": "^11.1.1",
		"nodemon": "^2.0.22",
		"typescript": "^5.0.4"
	},
	"release": {
		"branches": [
			"main"
		]
	}
}
